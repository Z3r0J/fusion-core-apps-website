    "clsx": "2.1.1",
    "framer-motion": "12.5.0",
    "lucide-react": "0.525.0",
    "tailwind-merge": "3.3.1",
    "zod": "4.0.5"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/react": "4.3.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
