    "lint-staged": "16.1.2",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-astro-organize-imports": "0.4.11",
    "prettier-plugin-tailwindcss": "0.6.14",
    "react": "19.1.0",
